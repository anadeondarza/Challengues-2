    "def es_multiplo(n):\n",
    "    list = []\n",
    "    list2 = [i for i in range(n)]\n",
    "    if i < 0:\n",
    "        return 0\n",
    "    else: \n",
    "        for i in list2:\n",
    "            if i % 3 == 0 & i % 5 == 0:\n",
    "                list.append(i)\n",
    "    counter = sum(list)\n",
    "    \n",
    "    return counter"
   ]
  }
