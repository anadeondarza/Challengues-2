   "source": [
    "SELECT\n",
    "    c.id AS category_id,\n",
    "    c.category,\n",
    "FROM categories AS c,\n",
    "Left join LATERAL (\n",
    "    SELECT\n",
    "        p.title,\n",
    "        p.views,\n",
    "        p.id,\n",
    "        p.category_id\n",
    "    FROM posts AS p\n",
    "    order by views DESC \n",
    "    LIMIT 2)\n",
    "  ON p.category_id = c.id"
   ]
  }
